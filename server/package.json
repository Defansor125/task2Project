{
  "dependencies": {
    "@types/express-session": "^1.18.1",
    "express": "^4.21.2",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.1",
    "handlebars": "^4.7.8"
  },
  "scripts": {
    "watch:sign-up": "tsc ./views/sign-up/sign-up.ts --outDir ./views/sign-up/ --watch",
    "watch:start-page": "tsc ./views/start-page/startpage.ts --outDir ./views/start-page --watch",
    "watch:profile": "tsc ./views/profile/profile.ts --outDir ./views/profile --watch",
    "start": "nodemon",
    "dev": "concurrently \"npm:start\" \"npm:watch:sign-up\" \"npm:watch:start-page\" \"npm:watch:profile\""
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/express-handlebars": "^6.0.2",
    "@types/node": "^22.13.11",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "type": "module"
}
